# Dinari Blockchain Environment Configuration
# Copy this file to .env and customize for your deployment

# RPC Authentication
RPC_USER=dinariuser
RPC_PASSWORD=CHANGE_THIS_PASSWORD

# Mining Configuration (for mining profile)
MINING_ADDRESS=D1YourMiningAddressHere
MINING_THREADS=4

# Network Configuration
# mainnet or testnet
DINARI_NETWORK=mainnet

# Data Directory
DATA_DIR=/data/dinari

# Logging
LOG_LEVEL=info
